class Human:
  #コンストラクタを定義
  def __init__(self, name, age):
    self.name = name
    self.age = age

  #メソッドを定義
  def set_name(self, name):
    self.name = name

  def show_name(self):
    print(self.name)

  def set_age(self, name):
    self.age = age

  def show_age(self):
    print(self.age)

#オブジェクト化
human = Human("HINOME", 27)

